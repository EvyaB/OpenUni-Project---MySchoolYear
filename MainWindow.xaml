<Window x:Class="MySchoolYear.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MySchoolYear"
        xmlns:views = "clr-namespace:MySchoolYear.View"
        mc:Ignorable="d"
        Title="MySchoolYear" Height="450" Width="800"
        Background = "{StaticResource WindowBackgroundBrush}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="53*"/>
        </Grid.ColumnDefinitions>
        <Image Margin="30,21,42,253" Source="Utilities/LogoImage.png" Stretch="Fill" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
